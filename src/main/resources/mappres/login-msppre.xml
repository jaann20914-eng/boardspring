<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  
<mapper namespace="Member">
   <select id="seletAll" resultType="com.keud.study.dto.MemberDTO">
		select * from member
   </select>
   
   <insert id="insert">
		insert into member values(member_seq.nextval,#{id},#{pw},#{name},#{email})
   </insert>
   
   <delete id="deleteId">
   	delete from pw=#{pw}, name=#{name}, email=#{email} where id=#{id}
   </delete>
   
   <update id="updateMember">
		update member set pw=#{pw}, name=#{name}, email=#{email} WHERE id =
		#{id}
   </update>
   
</mapper>